<!-- web 메모장으로 열기 -->
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
                      https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
         version="6.0">
<!--  MyServlet1  -->
    <servlet>
        <servlet-name>my-servlet1</servlet-name>
        <servlet-class>servlets.MyServlet1</servlet-class>
    </servlet>

    <!--mapping 검색-->
    <servlet-mapping>
        <servlet-name>my-servlet1</servlet-name>
        <url-pattern>/ex01</url-pattern>
    </servlet-mapping>

<!-- MyServlet2   -->
    <servlet>
        <servlet-name>my-servlet2</servlet-name>
        <servlet-class>servlets.MyServlet2</servlet-class>
    </servlet>

    <!--mapping 검색-->
    <servlet-mapping>
        <servlet-name>my-servlet2</servlet-name>
        <url-pattern>/ex02</url-pattern>
    </servlet-mapping>
</web-app>
